<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<TextView android:layout_width="fill_parent" android:padding="20dip"
		android:layout_gravity="center" android:textSize="24sp"
		android:layout_height="wrap_content" android:text="@string/enlace" />
	<EditText android:layout_width="fill_parent" android:id="@+id/text"
		android:layout_height="wrap_content" />
	<Button android:layout_width="fill_parent" android:id="@+id/enlace"
		android:singleLine="false" android:layout_height="wrap_content"
		android:text="@string/nota" />
	<Button android:layout_width="fill_parent" android:id="@+id/salir"
		android:singleLine="false" android:layout_height="wrap_content"
		android:text="@string/salir" />
</LinearLayout>
